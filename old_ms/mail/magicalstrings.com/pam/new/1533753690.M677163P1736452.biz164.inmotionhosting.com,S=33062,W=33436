                          <td class="ltBlock block_td" width="57%" valign="top" align="left" style="padding-right:15px; ">
                            <table class="deviceWidth" width="100%" border="0" cellspacing="0" cellpadding="0" style="background-color:#ffffff; font-family:Arial, Helvetica, sans-serif; font-size:12px; color:#000000; ">
